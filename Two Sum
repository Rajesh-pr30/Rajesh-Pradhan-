class Solution {
    public int[] twoSum(int[] nums, int target) {
        int len=nums.length;
        int i,j;
        int n=0;
        int result[]=new int[2];
        int s=0;
        for (i=0;i<len;i++)
     {
n=nums[i];
for(j=i+1;j<len;j++)
{
    s=n+nums[j];
    if(s==target)
    {
        result[0]=i;
        result[1]=j;
    }
}
     }
     return result;   
    }
}
